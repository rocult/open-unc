{
	"name": "@rbxts/open-unc",
	"version": "1.0.0",
	"description": "",
	"main": "out/init.lua",
	"scripts": {
		"build:rbx": "rbxtsc",
		"watch:rbx": "rbxtsc -w",
		"build:darklua": "darklua process out/init.luau dist/init.lua",
		"watch:darklua": "darklua process out/init.luau dist/init.lua -w",
		"build": "pnpm run build:rbx && pnpm run build:darklua",
		"watch": "concurrently \"pnpm run watch:rbx\" \"pnpm run watch:darklua\"",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"format": "prettier src -c",
		"format:fix": "prettier src --write"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"types": "out/index.d.ts",
	"files": [
		"out",
		"!**/*.tsbuildinfo"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.17.0",
		"@rbxts/compiler-types": "3.0.0-types.0",
		"@rbxts/types": "^1.0.813",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"concurrently": "^9.1.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-roblox-ts": "^0.0.36",
		"prettier": "^3.4.2",
		"roblox-ts": "^3.0.0",
		"typescript": "^5.7.2"
	}
}
